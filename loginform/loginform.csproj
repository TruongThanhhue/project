<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net8.0-windows</TargetFramework>
		<RootNamespace>loginform</RootNamespace>
		<Nullable>disable</Nullable>
		<UseWindowsForms>true</UseWindowsForms>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>

	<ItemGroup>
		<!-- Gói NuGet cho System.Data.SqlClient đã được thêm vào đây -->
		<PackageReference Include="System.Data.SqlClient" Version="4.8.6" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="Forgotpass.cs">
			<SubType>Form</SubType>
		</Compile>
		<Compile Update="Form1.cs">
			<SubType>Form</SubType>
		</Compile>
		<Compile Update="Login.cs">
			<SubType>Form</SubType>
		</Compile>
		<Compile Update="Recoverpass.cs">
			<SubType>Form</SubType>
		</Compile>
		<Compile Update="Signup.cs">
			<SubType>Form</SubType>
		</Compile>
		<Compile Update="ThongBaoTaoTaiKhoanThanhCong.cs">
			<SubType>Form</SubType>
		</Compile>
		<Compile Update="ThongBaoDatLaiMatKhauThanhCong.cs">
			<SubType>Form</SubType>
		</Compile>
	</ItemGroup>

</Project>
